<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow unsigned executable memory (required for some native modules) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Allow JIT compilation (required for V8/JavaScript engine) -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Allow outbound network connections (required for API calls) -->
    <key>com.apple.security.network.client</key>
    <true/>

    <!-- Allow reading user-selected files (for import/export) -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
</dict>
</plist>
